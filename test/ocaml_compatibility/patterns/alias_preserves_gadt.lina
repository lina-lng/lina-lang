(* Alias captures GADT-refined type correctly *)

type _ container =
  | IntBox : int -> int container
  | StrBox : string -> string container

let extract_boxed : type a. a container -> a = fun c ->
  match c with
  | (IntBox n as box) -> n
  | (StrBox s as box) -> s

let _ = print (extract_boxed (IntBox 42))
