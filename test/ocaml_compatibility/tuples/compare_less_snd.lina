(* Tuple: compare less by second element *)
(* Expected: ACCEPT - lexicographic comparison falls through to second *)

let int_cmp a b = if a < b then 0 - 1 else if a > b then 1 else 0
let p1 = (1, 2)
let p2 = (1, 3)
let c = Tuple.compare int_cmp int_cmp p1 p2
let _ = print (if c < 0 then 1 else 0)
