(* Expected: ACCEPT - loops return unit *)

let process () =
  let loop_result : unit =
    for i = 1 to 3 do
      ()
    done
  in
  let while_result : unit =
    while false do
      ()
    done
  in
  let _ = loop_result in
  let _ = while_result in
  42

let _ = print (process ())
