(** Test: Array of functions
    Expected: ACCEPT

    Verifies arrays can contain functions with proper typing. *)

let ops = Array.init 3 (fun i ->
  if i == 0 then (fun x -> x + 1)
  else if i == 1 then (fun x -> x * 2)
  else (fun x -> x - 1))

let f0 = Array.get_exn ops 0
let f1 = Array.get_exn ops 1

let _ = print (f0 10)
let _ = print (f1 10)
